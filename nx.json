{
  "npmScope": "contributors-img",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": ["build", "test", "lint"]
      }
    }
  },
  "projects": {
    "core": {
      "tags": ["lib"]
    },
    "webapp": {
      "tags": ["app"]
    },
    "api": {
      "tags": ["app"]
    }
  }
}
