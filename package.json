{
  "name": "contributors-img",
  "version": "1.0.0",
  "scripts": {
    "ng": "nx",
    "start": "node -r dotenv/config node_modules/.bin/nx serve",
    "build": "nx build",
    "build:all": "nx run-many --target build --projects api,webapp,worker --parallel",
    "build:all:staging": "yarn build:all --configuration staging",
    "build:all:production": "yarn build:all --configuration production",
    "test": "nx test",
    "test:ci": "nx run-many --target test --all --parallel",
    "format": "yarn prettier --write '.' && yarn rustfmt",
    "format:check": "yarn prettier --check '.' && yarn rustfmt --check",
    "rustfmt": "cargo fmt",
    "lint": "nx workspace-lint && nx lint && yarn format:check",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "update": "nx migrate @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2020 browser module main && node decorate-angular-cli.js",
    "workspace-generator": "nx workspace-generator",
    "e2e": "ng e2e",
    "firebase": "firebase"
  },
